<!DOCTYPE html>
<html lang="en">

<%- include("../partials/head",{title:"Home"}) %>

    <body>
        <%- include("../partials/header")%>
            <div class="layout">

                <main class="main_index">
                    <div class="divNav">
                        <a class="divNavA" href="./productos/listas-productos">Productos
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#FF0000"
                                    class="bi bi-chevron-double-right" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd"
                                        d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708" />
                                    <path fill-rule="evenodd"
                                        d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708" />
                            </svg>
                        </a>
                        <nav class="navHome">
                            <p>
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#FF0000"
                                    class="bi bi-cart-fill" viewBox="0 0 16 16">
                                    <path
                                        d="M0 1.5A.5.5 0 0 1 .5 1H2a.5.5 0 0 1 .485.379L2.89 3H14.5a.5.5 0 0 1 .491.592l-1.5 8A.5.5 0 0 1 13 12H4a.5.5 0 0 1-.491-.408L2.01 3.607 1.61 2H.5a.5.5 0 0 1-.5-.5M5 12a2 2 0 1 0 0 4 2 2 0 0 0 0-4m7 0a2 2 0 1 0 0 4 2 2 0 0 0 0-4m-7 1a1 1 0 1 1 0 2 1 1 0 0 1 0-2m7 0a1 1 0 1 1 0 2 1 1 0 0 1 0-2" />
                                </svg>
                                Mi Compra
                            </p>
                            <a href="/carrito"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#3E2723"
                                    class="bi bi-chevron-double-right" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd"
                                        d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708" />
                                    <path fill-rule="evenodd"
                                        d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708" />
                                </svg> Mis Productos
                            </a>
                            <a href=""><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#3E2723"
                                    class="bi bi-chevron-double-right" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd"
                                        d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708" />
                                    <path fill-rule="evenodd"
                                        d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708" />
                                </svg> Cupones
                            </a>
                            <a href=""><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#3E2723"
                                    class="bi bi-chevron-double-right" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd"
                                        d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708" />
                                    <path fill-rule="evenodd"
                                        d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708" />
                                </svg> Métodos de pago
                            </a>
                            <a href=""><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#3E2723"
                                    class="bi bi-chevron-double-right" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd"
                                        d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708" />
                                    <path fill-rule="evenodd"
                                        d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708" />
                                </svg> Envío
                            </a>
                            <a href=""><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#3E2723"
                                    class="bi bi-chevron-double-right" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd"
                                        d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708" />
                                    <path fill-rule="evenodd"
                                        d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708" />
                                </svg> Resumen
                            </a>
                        </nav>
                    </div>
                    <div class="contenedorHome">
                        <!-- Inicio Carousel-->
                        <div class="carousel_main">
                            <div id="carouselExampleFade" class="carousel slide carousel-fade m_edia2">
                                <div class="carousel-inner">
                                    <div class="carousel-item active">
                                        <img src="../../../images/carousel1.jpg" class="d-block w-100"
                                            alt="Imagen Carousel 1">
                                    </div>
                                    <div class="carousel-item">
                                        <img src="../../../images/carousel2.jpg" class="d-block w-100"
                                            alt="Imagen Carousel 2">
                                    </div>
                                    <div class="carousel-item">
                                        <img src="../../../images/carousel3.jpg" class="d-block w-100"
                                            alt="Imagen Carousel 3">
                                    </div>
                                </div>
                                <button class="carousel-control-prev" type="button"
                                    data-bs-target="#carouselExampleFade" data-bs-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                    <span class="visually-hidden">Previous</span>
                                </button>
                                <button class="carousel-control-next" type="button"
                                    data-bs-target="#carouselExampleFade" data-bs-slide="next">
                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                    <span class="visually-hidden">Next</span>
                                </button>
                            </div>
                            <div id="carouselExampleIndicators" class="carousel slide m_edia">
                                <div class="carousel-indicators">
                                    <button type="button" data-bs-target="#carouselExampleIndicators"
                                        data-bs-slide-to="0" class="active" aria-current="true"
                                        aria-label="Slide 1"></button>
                                    <button type="button" data-bs-target="#carouselExampleIndicators"
                                        data-bs-slide-to="1" aria-label="Slide 2"></button>
                                    <button type="button" data-bs-target="#carouselExampleIndicators"
                                        data-bs-slide-to="2" aria-label="Slide 3"></button>
                                </div>
                            </div>
                        </div>
                        <!-- Fin Carousel -->
                        <div>
                            <p class="main_p">ÚLTIMAS NOVEDADES</p>
                        </div>
                        <div class="cardHome">
                            <% let count = 0; %>
                            <% products.forEach(p => { %>
                            <% if (count < 4) { %>
                            <article class="articleHome">
                                <a href="/productos/detalle/<%= p.id %>" >
                                    <div>
                                        <img src="../../../images/<%= p.image %>" alt="ryzen">
                                    </div>
                                    <h5> <%= p.description %> </h5>
                                    <p>$ <%= p.price %></p>
                                </a>
                            </article>
                            <% count++; %>
                            <% } %>
                            <% }) %>
                        </div>
                    </div>

                </main>

            </div>
            <%- include("../partials/footer")%>
                <%- include("../partials/scripts")%>
    </body>

</html>